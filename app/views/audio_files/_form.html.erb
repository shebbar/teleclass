<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= simple_form_for(@audio_file) do |f| %>
      <%= f.error_notification %>

      <div class="form-inputs">
				<%= f.input :telephone_code %>
        <%= f.input :title %>
      </div>
      <div class="form-actions">
        <%= f.button :submit, class: "btn btn-primary" %>
      </div>
    <% end %>
  </div>
</div>
